import java.util.Scanner;
public class sumfunc {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//int a=0;
	int	sum=0;
Scanner sc =new Scanner(System.in);

for(int i =0;i<=10;i++) {
	if(i%2==0) {
		sum=sum+(i*i);
		
}

}
System.out.println("sum of numbers is " +sum);

}

}
