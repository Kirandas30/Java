import java.util.Scanner;

public class swapwo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a;
		int b;
Scanner sc = new Scanner(System.in);
System.out.print("Enter two numbers");
 a =sc.nextInt();
 b=sc.nextInt();
a=b+a;
b=a-b;
a=a-b;
System.out.print("a="+a);
System.out.print("b="+b);
}
}
