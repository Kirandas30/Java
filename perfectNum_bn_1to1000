package practiceQuestions;

public class pq1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub


		

    for(int i = 1;i<=1000;i++) 
    {
    	int sum=0;		
	  for(int j = 1;j<=i;j++)
	{
	           if (i%j==0 && i!=j) 
		         sum = sum+i;
		
	}
	          if(i==sum) 
                System.out.println( i);
        
       
	 }
  }
	}


